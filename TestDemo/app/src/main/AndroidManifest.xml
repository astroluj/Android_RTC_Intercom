<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.astroluj.intercom.testdemo">

    <application
        android:name=".app.RTCApp"
        android:allowBackup="true"
        android:fullBackupContent="false"
        android:icon="@android:mipmap/sym_def_app_icon"
        android:label="@string/application_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.DarkActionBar">

        <activity
            android:name="com.astroluj.intercom.testdemo.PermissionActivity"
            android:exported="true"
            android:noHistory="true"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.BROWSABLE" />
                <data
                    android:host="webrtc.org"
                    android:scheme="http" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.astroluj.intercom.testdemo.PartnerDetectorActivity"
            android:exported="false"
            android:noHistory="true"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar" />

        <activity
            android:name="com.astroluj.intercom.testdemo.RTCActivity"
            android:exported="false"
            android:noHistory="true"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar" />

    </application>
</manifest>